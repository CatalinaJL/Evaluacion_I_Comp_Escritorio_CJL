<Window x:Class="Evaluacion_I_Comp_Escritorio_CJL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Evaluacion_I_Comp_Escritorio_CJL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="False" Margin="0,0,0,391">
            <Menu Width="789" Height="30" DockPanel.Dock="Top">
                <MenuItem Header="Equipo" x:Name="itemEquipo">
                    <MenuItem Header="Agregar" x:Name="Agregar" Click="Agregar_Click"/>
                    <MenuItem Header="Listar Todos" x:Name="ListarTodos" Click="ListarTodos_Click"/>
                </MenuItem>
                <MenuItem Header="Información" x:Name="itemInformacion">
                    <MenuItem Header="Acerca de" x:Name="AcercaDe" Click="AcercaDe_Click"/>
                </MenuItem>
            </Menu>
        </DockPanel>

    </Grid>
</Window>
